{
    "plugins": ["plugins/markdown", "plugins/summarize"],
    "source": {
        "include": ["server/"],
        "exclude": ["server/node_modules", "server/samples", "server/bin"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs/code"
    }
}
